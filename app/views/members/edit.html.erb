<%= form_for @member do |f| %>

    <label>Region</label>
    <%= f.text_field :region, class: "col-md-2 col form-control" %>

    <label>Office</label>
    <%= f.text_field :office, class: "col-md-2 col form-control" %>

    <label>Headshot</label>
    <%= f.file_field :profpic, class: "col-md-2 col form-control" %>

    <label>Name</label>
    <%= f.text_field :name, class: "col-md-2 col form-control" %>

    <label>Designation</label>
    <%= f.text_field :designation, class: "col-md-2 col form-control" %>

    <label>Facebook</label>
    <%= f.text_field :facebook, class: "col-md-2 col form-control" %>

    <label>Linkedin</label>
    <%= f.text_field :linkedin, class: "col-md-2 col form-control" %>

    <label>Twitter</label>
    <%= f.text_field :twitter, class: "col-md-2 col form-control" %>

    <label>Instagram</label>
    <%= f.text_field :ig, class: "col-md-2 col form-control" %>

    <label>Email</label>
    <%= f.text_field :email, class: "col-md-2 col form-control" %>

    <%= f.submit class: "btn btn-success" %>
    <%= link_to "Back", new_event_path, :class => "btn btn-outline-primary" %>

<% end %>